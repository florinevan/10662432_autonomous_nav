<?xml version="1.0" encoding="utf-8"?>	

<launch>
  <arg name="robot_prefix" default="turtlebot1"/>

  <node pkg="pcd_scan_registration" type="pcd_scan_registration" respawn="false" name="pcd_scan_registration_$(arg robot_prefix)" output="screen">
		<param name = "_name" value = "/"/>
		<param name = "pcd_colored_topic_name" value = "/$(arg robot_prefix)/depth/pcd_colored"/>		
  </node>
  
  
</launch>
